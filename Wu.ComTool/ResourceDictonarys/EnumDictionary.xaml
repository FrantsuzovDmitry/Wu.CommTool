<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:model="clr-namespace:Wu.ComTool.Models"
                    xmlns:port="clr-namespace:System.IO.Ports;assembly=System.IO.Ports"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!--  波特率枚举  -->
    <ObjectDataProvider x:Key="NameBaudRate"
                        MethodName="GetNames"
                        ObjectType="{x:Type sys:Enum}">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="model:BaudRate" />
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>

    <ObjectDataProvider x:Key="ValueBaudRate"
                        MethodName="GetValues"
                        ObjectType="{x:Type sys:Enum}">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="model:BaudRate" />
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>

    <!--  校验  -->
    <ObjectDataProvider x:Key="ValueParity"
                        MethodName="GetValues"
                        ObjectType="{x:Type sys:Enum}">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="port:Parity" />
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>

</ResourceDictionary>